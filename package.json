{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "put-io",
  "title": "put.io",
  "description": "Raycast extension for managing files, transfers, and history events in your put.io account.",
  "icon": "putio.png",
  "author": "altaywtf",
  "owner": "putio",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "files",
      "title": "Your Files",
      "description": "View your files in put.io",
      "mode": "view",
      "keywords": [
        "putio",
        "files"
      ]
    },
    {
      "name": "search-files",
      "title": "Search in put.io",
      "description": "Search among your files in put.io",
      "mode": "view",
      "keywords": [
        "putio",
        "files",
        "search"
      ]
    },
    {
      "name": "new-transfer",
      "title": "New transfer",
      "description": "Add new transfer to put.io",
      "mode": "view",
      "keywords": [
        "putio",
        "transfer",
        "new"
      ]
    },
    {
      "name": "transfers",
      "title": "Transfers",
      "description": "View your transfers in put.io",
      "mode": "view",
      "keywords": [
        "putio",
        "files"
      ]
    }
  ],
  "preferences": [
    {
      "name": "token",
      "title": "App-specific password",
      "type": "password",
      "placeholder": "Get this from put.io/raycast",
      "description": "App-specific password used for authenticating with put.io API.",
      "required": true
    }
  ],
  "dependencies": {
    "@putdotio/api-client": "^8.36.1",
    "@raycast/api": "^1.52.1",
    "@raycast/utils": "^1.4.8",
    "filesize": "^10.0.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
